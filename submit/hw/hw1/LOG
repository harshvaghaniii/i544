***** revStrings() *****
> revStrings(['hello', 'world'])
[ 'olleh', 'dlrow' ]
> revStrings(['madam', 'racecar'])
[ 'madam', 'racecar' ]
> revStrings(['1234'])
[ '4321' ]
> revStrings([])
[]

***** getCapitalizedWords() *****
> getCapitalizedWords(' Hello, world')
[ 'Hello' ]
> getCapitalizedWords(' A Big--Boy')
[ 'Big', 'Boy' ]
> getCapitalizedWords('A B C ')
[]
> getCapitalizedWords('')
[]

***** getCamelCasedWords() *****
> getCamelCasedWords(' Helloworld')
[]
> getCamelCasedWords(' A BigBoy Barracuda--camelCased')
[ 'BigBoy', 'camelCased' ]
> getCamelCasedWords('A B C ')
[]
> getCamelCasedWords('')
[]

***** upDown1n1() *****
> upDown1n1(3)
[ 1, 2, 3, 2, 1 ]
> upDown1n1(5)
[
  1, 2, 3, 4, 5,
  4, 3, 2, 1
]
> upDown1n1(1)
[ 1 ]

***** isPermutation() *****
> isPermutation([1, 3, 2], [1, 2, 3])
true
> isPermutation([2, 3, 4], [2, 3, 4])
true
> isPermutation([2, 3, 4], [2, 3, 1])
false
> isPermutation([2], [2])
true
> isPermutation([2, 1], [1, 2, 3])
false
> isPermutation([1], [])
false
> isPermutation([], [])
true

***** pow() *****
> pow(2, 5)
32
> pow(-2, 3)
-8
> pow(-5, 5)
-3125
> pow(-5, 0)
1

***** tetrate() *****
> tetrate(2, 4)
65536
> tetrate(2, 5)
Infinity
> tetrate(5, 2)
3125

