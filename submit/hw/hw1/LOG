[ [ 1 ], [ 2 ] ]
> nTuples([1, 2], 2)
[ [ 1, 2 ] ]
> nTuples([1, 2, 3, 4, 5, 6], 3)
[ [ 1, 2, 3 ], [ 4, 5, 6 ] ]
> nTuples(['a', 'b', 'c', 'd', 'e', 'f'], 3)
[ [ 'a', 'b', 'c' ], [ 'd', 'e', 'f' ] ]
> nTuples(['a', 'b', 'c', 'd', 'e', 'f', 'h', 'i'], 4)
[ [ 'a', 'b', 'c', 'd' ], [ 'e', 'f', 'h', 'i' ] ]
> nTuples([['a', 'b'], ['c'], ['d'], ['e'], ['f', 'h'], ['i']], 3)
[
  [ [ 'a', 'b' ], [ 'c' ], [ 'd' ] ],
  [ [ 'e' ], [ 'f', 'h' ], [ 'i' ] ]
]
> nTuples([], 1)
[]
> nTuples([], 10)
[]

